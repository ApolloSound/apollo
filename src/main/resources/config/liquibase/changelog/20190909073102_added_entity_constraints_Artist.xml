<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Artist.
    -->
    <changeSet id="20190909073102-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="artist_id"
                                 baseTableName="album_artist"
                                 constraintName="fk_artist_album_artist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artist"/>

        <addForeignKeyConstraint baseColumnNames="album_id"
                                 baseTableName="album_artist"
                                 constraintName="fk_artist_album_album_id"
                                 referencedColumnNames="id"
                                 referencedTableName="album"/>

    </changeSet>

</databaseChangeLog>
